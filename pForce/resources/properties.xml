<resources>

    <properties>
        <property id="riderWeight_prop" type="float">78.0</property>
        <property id="riderHeight_prop" type="number">192</property>
        <property id="bikeWeight_prop" type="float">8.0</property>  
  
        <property id="temp_prop" type="number">8</property>
        <property id="windHeading_prop" type="number">90</property>
        <property id="windSpeed_prop" type="float">3.0</property>
                
        <property id="average_prop" type="number">3</property>                  
        <property id="ftpPower_prop" type="number">295</property>
        <property id="z2_prop" type="number">56</property>
        <property id="z3_prop" type="number">76</property>
        <property id="z4_prop" type="number">91</property>
        <property id="z5_prop" type="number">106</property>
        <property id="z6_prop" type="number">121</property>
        <property id="z7_prop" type="number">151</property>
     </properties>

    <strings>
       <string id="riderWeight_title">Rider weight in full riding gear (40-160 kg)</string>
       <string id="riderHeight_title">Rider height (130-220 cm)</string>
       <string id="bikeWeight_title">Bike weight with bottles and all extras (5 - 17 kg)</string>              
       
       <string id="temp_title">Outside temperature (-15 - 40 Deg Celcius)</string>
       <string id="windHeading_title">Wind heading (0 - 359 Degrees)</string>       
       <string id="windSpeed_title">Wind speed (0 - 30 metres/sec)</string>    
                            
       <string id="average_title">Power Averaging Period (1/3/5/10 sec)</string>
       <string id="ftpPower_title">FTP (watts)</string>
       <string id="z2_title">Zone 2 Lower Limit (0-200% of FTP)</string>
       <string id="z3_title">Zone 3 Lower Limit (0-200% of FTP)</string>
       <string id="z4_title">Zone 4 Lower Limit (0-200% of FTP)</string>
       <string id="z5_title">Zone 5 Lower Limit (0-200% of FTP)</string>
       <string id="z6_title">Zone 6 Lower Limit (0-200% of FTP)</string>
       <string id="z7_title">Zone 7 Lower Limit (0-200% of FTP)</string>
       
       <string id="riderWeight_error">Rider weight must be 40-160 kg</string>
       <string id="riderHeight_error">Rider height must be 130-220 cm</string>
       <string id="bikeWeight_error">Bike weight must be 5-17 kg</string>
       
       <string id="temp_error">Temperature must be -15 - 40 deg Celcius</string>
       <string id="windHeading_error">Wind heading must be 0-359 degrees</string>
       <string id="windSpeed_error">Wind speed must be 0-30 metres/sec</string>

       <string id="zone_error">Zone value must be between 0-200% of FTP</string>
       <string id="ftp_error">FTP should be between 50-400 watts</string>   
       
       <string id="average_1s_title">1 sec (instant)</string> 
       <string id="average_3s_title">3 sec</string>      
       <string id="average_10s_title">10 sec</string>  
       <string id="average_30s_title">30 sec</string>   
       
    </strings>
 
    <settings>
        <setting propertyKey="@Properties.riderWeight_prop" title="@Strings.riderWeight_title">
            <settingConfig type="numeric" required="true" min="40" max="160" errorMessage="@Strings.riderWeight_error"/>
        </setting>

        <setting propertyKey="@Properties.riderHeight_prop" title="@Strings.riderHeight_title">
            <settingConfig type="numeric" required="true" min="130" max="220" errorMessage="@Strings.riderHeight_error"/>
        </setting>

        <setting propertyKey="@Properties.bikeWeight_prop" title="@Strings.bikeWeight_title">
            <settingConfig type="numeric" required="true" min="5" max="17" errorMessage="@Strings.bikeWeight_error"/>
        </setting>

        <setting propertyKey="@Properties.temp_prop" title="@Strings.temp_title">
            <settingConfig type="numeric" required="true" min="-15" max="40" errorMessage="@Strings.temp_error"/>
        </setting>

        <setting propertyKey="@Properties.windHeading_prop" title="@Strings.windHeading_title">
            <settingConfig type="numeric" required="true" min="0" max="359" errorMessage="@Strings.windHeading_error"/>
        </setting>
 
         <setting propertyKey="@Properties.windSpeed_prop" title="@Strings.windSpeed_title">
            <settingConfig type="numeric" required="true" min="0" max="30" errorMessage="@Strings.windSpeed_error"/>
        </setting>
          
        <setting propertyKey="@Properties.average_prop" title="@Strings.average_title">
            <settingConfig type="list" required="true">
            		<listEntry value="1">@Strings.average_1s_title</listEntry>            	
            		<listEntry value="3">@Strings.average_3s_title</listEntry>
            		<listEntry value="10">@Strings.average_10s_title</listEntry>
              	<listEntry value="30">@Strings.average_30s_title</listEntry>
       		</settingConfig>
        </setting>
        
        <setting propertyKey="@Properties.ftpPower_prop" title="@Strings.ftpPower_title">
            <settingConfig type="numeric" required="true" min="50" max="400" errorMessage="@Strings.ftp_error"/>
        </setting>
        <setting propertyKey="@Properties.z2_prop" title="@Strings.z2_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>
        <setting propertyKey="@Properties.z3_prop" title="@Strings.z3_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>
        <setting propertyKey="@Properties.z4_prop" title="@Strings.z4_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>
        <setting propertyKey="@Properties.z5_prop" title="@Strings.z5_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>
        <setting propertyKey="@Properties.z6_prop" title="@Strings.z6_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>
        <setting propertyKey="@Properties.z7_prop" title="@Strings.z7_title">
            <settingConfig type="numeric" required="true" min="0" max="200" errorMessage="@Strings.zone_error"/>
        </setting>


    </settings>
</resources>
